# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB
MONGODB_URI=mongodb://localhost:27017/survey-saas

# Google AI / Gemini (required for POST /api/surveys/:surveyId/analyze)
# Get your key from https://aistudio.google.com/apikey (free tier available)
GOOGLE_API_KEY=
# Optional: model for analysis (default: gemini-1.5-flash)
# GOOGLE_AI_MODEL=gemini-1.5-flash

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRE=7d

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
